{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "baseUrl": ".",
    "paths": {
      "tls-notary-shared": [
        "../shared/dist"
      ],
      "tls-notary-shared/*": [
        "../shared/dist/*"
      ]
    },
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmit": false,
    "jsx": "react",
    "sourceMap": true,
    "inlineSourceMap": false,
    "inlineSources": false
  },
  "include": [
    "src/app.tsx",
    "src/worker.ts"
  ]
}